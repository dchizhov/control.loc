name = Feature View All Tasks Facet
description = Provides search_api_view called "All tasks by solr" with facet block to sort tasks by task topic
core = 7.x
package = Features View
version = 7.x-1.0
dependencies[] = facetapi
dependencies[] = search_api
dependencies[] = search_api_solr
dependencies[] = strongarm
dependencies[] = views
features[ctools][] = facetapi:facetapi_defaults:1
features[ctools][] = strongarm:strongarm:1
features[ctools][] = views:views_default:3.0
features[facetapi][] = search_api@tasks::field_task_topic
features[facetapi][] = search_api@tasks:block:field_task_topic
features[features_api][] = api:2
features[search_api_index][] = tasks
features[search_api_server][] = task_search_server
features[variable][] = facetapi:block_cache:search_api@tasks
features[variable][] = search_api_facets_search_ids
features[variable][] = search_api_solr_site_hash
features[views_view][] = all_tasks_solr
features_exclude[dependencies][ctools] = ctools
features_exclude[dependencies][entity] = entity
